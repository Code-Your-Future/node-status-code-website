  <header class='header'>
    <h3>All Codes</h3>
  </header>
  <div class='container'>
    {{#each content}}
    <div class='items'>
      <a target = "blank" href="{{rootEndPoint}}{{code}}">
        <figure>
          <img src="{{image}}">
        </figure>
      </a>
      <div class='detail'>
        <h4><em> {{code}} </em></p>
        <p><em> {{phrase}} </em></p>
      </div>
    </div>
    {{/each}}
  </div>  
